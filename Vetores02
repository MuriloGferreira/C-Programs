#include <stdio.h>
#include<stdlib.h>
int main() {
  float nota[5], media=0;
  int i, soma=0, cont=0;

  for(i=0; i<5; i++){
  printf("Digite a nota:");
  scanf("%f", &nota[i]);
  }
for(i=0; i<5; i++){
   soma+=nota[i];
   media=soma/5;
 }
printf("media = %f", media);

for(i=0; i<5; i++){
if(nota[i]>media){
  cont++;
    }
}
printf("\nEstao acima da media %d alunos", cont);

return 0;
}
